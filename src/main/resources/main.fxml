<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<VBox alignment="CENTER" spacing="20" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.papeleria.inventariopapeleria.controller.MainController" stylesheets="@styles.css">
    <Button text="Agregar Producto" onAction="#abrirAgregarProducto" styleClass="button"/>
    <Button text="Eliminar Producto" onAction="#abrirEliminarProducto" styleClass="button"/>
    <Button text="Actualizar Producto" onAction="#abrirActualizarProducto" styleClass="button"/>
    <Button text="Ventas" onAction="#abrirVentas" styleClass="button"/>
    <Button text="Inventario" onAction="#abrirInventario" styleClass="button"/>
</VBox>